  "repository": "eclipse-iceoryx/iceoryx2",
  "version": "0.8.0",
  "analysis_date": "2025-12-30",
  "version_changes": {
    "from_version": "0.7.0",
    "to_version": "0.8.0",
    "summary": "Massive increase in attribute usage due to FFI layer expansion. #[repr] increased 160% (197->512), #[no_mangle] introduced (602), #[derive] increased 52% (658->998), #[cfg] increased 140% (150->360). #![no_std] usage dramatically increased (2->23) for no_std support.",
    "key_changes": [
      "Directory structure changed: iceoryx2-bb-*-0.7.0/ -> iceoryx2-bb/*/",
      "#[derive] increased from 658 to 998 (+52%)",
      "#[cfg] increased from 150 to 360 (+140%)",
      "#[repr] increased from 197 to 512 (+160%)",
      "#[repr(C)] increased from 160 to 393 (+146%)",
      "#[no_mangle] introduced: 602 instances for FFI exports",
      "#[allow] increased from 62 to 142 (+129%)",
      "#![no_std] increased from 2 to 23 (+1050%) for no_std support",
      "#[doc] increased from 87 to 156 (+79%)",
      "#[inline] increased from 23 to 32 (+39%)",
      "#[test] decreased from 1463 to 960 (-34%)",
      "#[repr(transparent)] increased from 18 to 35 (+94%)"
    ]
  },
  "summary": "FLS Chapter 13 defines attributes as general, free-form metadata interpreted based on name, convention, language, and tool. iceoryx2 v0.8.0 makes extensive use of built-in attributes with major increases due to the FFI layer. Key additions include #[no_mangle] (602 instances) for C bindings and expanded #[repr(C)] usage (393 instances) for FFI type compatibility.",
  "statistics": {
    "derive_attributes": 998,
    "cfg_attributes": 360,
    "repr_attributes": 512,
    "test_attributes": 960,
    "no_mangle_attributes": 602,
    "allow_attributes": 142,
    "doc_attributes": 156,
    "inline_attributes": 32,
    "repr_c_attributes": 393,
    "repr_transparent_attributes": 35,
    "no_std_attributes": 23,
    "should_panic_attributes": 36,
    "non_exhaustive_attributes": 10,
    "must_use_attributes": 8,
    "ignore_attributes": 5,
    "macro_export_attributes": 5
  },
